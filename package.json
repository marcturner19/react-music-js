{
  "name": "react-music-js",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@npmcorp/pui-css-colors": "^6.4.0",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.0.2",
    "pui-css-colors": "^8.3.1",
    "pui-css-tables": "^8.3.1",
    "pui-react-buttons": "^8.3.1",
    "pui-react-dropdowns": "^8.3.1",
    "pui-react-iconography": "^8.3.1",
    "pui-react-inputs": "^8.3.1",
    "pui-react-modals": "^8.3.1",
    "pui-react-table": "^8.3.1",
    "pui-react-tools": "^2.0.5",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-router": "^4.1.1",
    "react-scripts": "1.0.10",
    "whatwg-fetch": "^2.0.3"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "engines": {
    "node": "^8.1.2",
    "npm": "5.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.6.0",
    "pivotal-js-jasmine-matchers": "^0.1.1",
    "react-svg-loader": "^1.1.1",
    "webpack": "^3.1.0"
  }
}
